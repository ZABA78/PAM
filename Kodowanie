//kodowanie

public class Main {
    public static void main(String[] args) {
    String napis = "kodowanie";
    System.out.println("Teskt: " + napis);
    System.out.print("Ciąg bajtów");
    byte[] kod = napis.getBytes();
    for(byte n:kod)
        System.out.print(" "+n+" ");
    System.out.println(("\b."));


    }
}
